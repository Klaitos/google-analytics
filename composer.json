{
    "name": "widop/google-analytics",
    "description": "Google certificate-based authentication in server-to-server interactions with google analytics",
    "keywords": [ "google", "analytics", "server" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Widop",
            "email": "contact@widop.com"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/petrepatrasc/WidopHttpAdapterBundle.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "widop/http-adapter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Widop\\GoogleAnalytics": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
